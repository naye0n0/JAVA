package gnuvill;

public class ex11 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
        
        boolean isCar = false;
        boolean isHaveHome = false;
        boolean isGoldSpoon = false;
        boolean isJob = false;
        
        int asset = 17000;
         
        int grade = myGrade(isCar,isHaveHome,isGoldSpoon,isJob,asset);
        
        System.out.println("your grade is:"+grade);
     }
     
     static int myGrade(boolean isCar,boolean isHaveHome,
    		 boolean isGoldSpoon, boolean isJob,int asset){
         
         //1~9
         int grade = 0;
         
         if(isGoldSpoon){
        	 grade=1;
         }else if(isHaveHome) {
        		 return grade =2;
    	 }else if(isCar) {
    		 return grade =3;
    	 }else if(isJob) {
    		 return grade = 4;
    	 }else {
    		 if(0<=asset && asset<17000) {
    			 return 9;
    		 }else if(17000<=asset && asset < 100000) {
    			 return 8;
    		 }else if(100000<=asset && asset<500000) {
    			 return 7;
    		 }
    	 }
		return grade;
         
         }
		
         
     }

